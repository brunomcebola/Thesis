{% extends "views/base.jinja" %}
{% block title %}
  Page Not Found
{% endblock title %}
{% block body %}
  <div class="container">
    <div class="row">
      <div class="col-12 text-center mt-5">
        <img src="{{ url_for('gui.static', filename='images/500.svg') }}"
             alt="500"
             class="img-fluid" />
      </div>
    </div>
    <div class="row">
      <div class="col-12 text-center mt-5">
        <a href="javascript:window.location.reload()">
          <p>
            <i class="fas fa-rotate-right"></i> Try again
          </p>
        </a>
      </div>
    </div>
  </div>
{% endblock body %}
